{
  "source": {
    "include": ["./"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|out/|.github/|bijou.js)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/docs",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "light",
      "create_style": "pre, pre * {font-family: monospace !important;} .tooltip * {color: black !important}",
      "add_scripts": "document.querySelector('.os-content-arrange').remove();var els = document.querySelectorAll(\"ul.accordion-content a\")\r\nfor (var i = 0; i < els.length; i++){\r\n   els[i].innerText = els[i].innerText.replace(/^exports\\./, \"\")\r\n}",
      "add_script_path": [
        {
          "href": "https://unpkg.com/bijou.js@v6.2.0",
          "type": "module"
        }
      ],
      "menu": [
        {
          "title": "Home",
          "link": "https://bijou.js.org"
        },
        {
          "title": "Github",
          "link": "https://github.com/bijou-js/bijou.js",
          "target": "_blank"
        },
        {
          "title": "Functions in Bijou",
          "link": "https://bijou.js.org/docs/bijou.html"
        }
      ],
      "meta": [
        {
          "name": "author",
          "content": "Explosion-Scratch"
        },
        {
          "name": "description",
          "content": "Bijou.js documentation: Everything function in Bijou.js documented!"
        },
        {
          "name": "keywords",
          "content": "Bijou.js, Docs, documentation, library, JS, HTML, CSS"
        },
        {
          "name": "theme-color",
          "content": "#008080"
        },
        {
          "property": "twitter:card",
          "content": "summary_large_image"
        },
        {
          "property": "twitter:url",
          "content": "https://bijou.js.org/docs"
        },
        {
          "property": "twitter:title",
          "content": "Bijou.js docs"
        },
        {
          "property": "og:title",
          "content": "Bijou.js docs"
        },
        {
          "property": "og:site_name",
          "content": "Bijou.js"
        },
        {
          "property": "og:description",
          "content": "Bijou.js documentation: Every function in Bijou.js is documented here!"
        },
        {
          "property": "og:image",
          "content": "https://media.discordapp.net/attachments/795728683548540929/812363131153809418/bijou.png?width=628&height=401"
        },
        {
          "property": "twitter:image",
          "content": "https://media.discordapp.net/attachments/795728683548540929/812363131153809418/bijou.png?width=628&height=401"
        },
        {
          "property": "twitter:description",
          "content": "Bijou.js documentation: Every function in Bijou.js is documented here!"
        },
        {
          "name": "language",
          "content": "English"
        },
        {
          "name": "revisit-after",
          "content": "1 days"
        }
      ],
      "add_style_path": [
        {
          "rel": "shortcut icon",
          "type": "image/png",
          "href": "https://bijou.js.org/favicon.png"
        }
      ]
    }
  }
}
